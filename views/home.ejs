  <body>
    <%- include("./partials/sliderbar.ejs"); %>
    <section class="overlay"></section>
    <% if (page == "age") { %>
    <%- include("./partials/age.ejs"); %>
    <% } else if (page=="death") {%>
        <%- include("./partials/death.ejs"); %>
        <% } else if (page =="ageVsEjec"){%>
          <%- include("./partials/ageVsEject.ejs"); %>
          <% } else if (page =="PateletsVsSerum"){%>
            <%- include("./partials/PlateletsVsSerum.ejs"); %>
            <% } else if (page =="summary"){%>
              <%- include("./partials/summary.ejs",{a,sc,ss,ef,p,cp}); %>
              <% } else if (page =="binomial"){%>
                <%- include("./partials/binomial.ejs",{x,n,p,pp}); %>
                <% } else if (page =="prediction"){%>
                  <%- include("./partials/prediction.ejs",{p}); %>
                  <% } else if (page =="confidence"){%>
                    <%- include("./partials/confidence.ejs",{data}); %>
                    <% } else if (page =="adddata"){%>
                      <%- include("./partials/adddata.ejs"); %>
                      <% } else {%>
                        <%- include("./partials/home.ejs"); %>
                        <%} %>
    <script src="/js/slider.js">
    </script>
